@import '../../common/common.wxss';
.content{
  width:100%;
}
.address{
  width:100%;
  border-bottom:10px solid #eee;
}

.address-left{
  line-height: 55rpx;
  width:550rpx;
  padding:15rpx 20rpx
}
.address-right{
  width:100rpx;
  margin:10rpx 0;
  border-left:1px solid #ccc;
  padding:0 10rpx
}
.address-right button{
  font-size:25rpx;
  padding:8rpx;
  margin:10rpx auto;
  display:block;
  width:75%;
  line-height:36rpx
}
.address-left text{
  display:block;
  width:100%
}
.but{
  width:90%;
  display:block;
  margin:40rpx auto
}
.addBtn{
  width:100%;
  padding-top:30rpx
}